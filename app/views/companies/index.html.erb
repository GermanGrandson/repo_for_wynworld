<p id="notice"><%= notice %></p>

<h1>Listing Companies</h1>


<%= @companies.inspect %>





<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street1</th>
      <th>Street2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Description</th>
      <th>Website</th>
      <th>Hp</th>
      <th>Org Type</th>
      <th>Latitude</th>
      <th>Longitude</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.street1 %></td>
        <td><%= company.street2 %></td>
        <td><%= company.city %></td>
        <td><%= company.state %></td>
        <td><%= company.zip %></td>
        <td><%= company.description %></td>
        <td><%= company.website %></td>
        <td><%= company.hp %></td>
        <td><%= company.orgtype %></td>
        <td><%= company.latitude %></td>
        <td><%= company.longitude %></td>
        <td><%#= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%#= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
