$(function() {

 $('#search_results').html("");

<% if @user.nil? %>
  $('#search_results').append("No Results");
<% else %>
   <% @user.each do |user| %>
     var a_user = '<a href="/users/<%= user.id %>"><%= user.fname << " " << user.lname %></a>';
     $('#search_results').append(a_user);
     $('#search_results').append("<br>");
   <% end %>
<% end %>


});
